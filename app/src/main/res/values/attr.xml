<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CoverImageView">
        <attr name="civ_dynamic_size" format="boolean" />
        <attr name="civ_shadow_position">
            <enum name="top" value="1" />
            <enum name="bottom" value="-1" />
            <enum name="both" value="2" />
            <enum name="none" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StatusBarView">
        <attr name="sbv_translucent_mode" format="boolean" />
        <attr name="sbv_fill_in_mode" format="boolean" />
        <attr name="sbv_init_alpha" format="float" />
        <attr name="sbv_darker_alpha" format="float" />
    </declare-styleable>

    <attr format="color" name="colorBgItemDrawer"/>
    <attr format="color" name="colorItemDrawer"/>
</resources>